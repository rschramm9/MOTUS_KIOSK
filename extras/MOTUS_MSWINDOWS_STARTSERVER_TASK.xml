<?xml version="1.0" encoding="UTF-16"?>
<Task version="1.2" xmlns="http://schemas.microsoft.com/windows/2004/02/mit/task">
  <RegistrationInfo>
    <Date>2022-11-12T15:09:06.8909151</Date>
    <Author>AHNC_MOTUS1\Admin</Author>
    <Description>Task to run the Motus Kiosk Shiny WebApp using R when the computer boots. This is done via the MOTUS_MSWINDOWS_STARTSERVER.bat file from Users/MOTUS_USER/Projects/MOTUS_KIOSK.   There is a Log file kept in the Logs directory of that project.</Description>
    <URI>\MOTUS_MSWINDOWS_STARTSERVER_TASK</URI>
  </RegistrationInfo>
  <Triggers>
    <BootTrigger>
      <Enabled>true</Enabled>
    </BootTrigger>
  </Triggers>
  <Principals>
    <Principal id="Author">
      <UserId>Admin</UserId>
      <LogonType>Password</LogonType>
      <RunLevel>LeastPrivilege</RunLevel>
    </Principal>
  </Principals>
  <Settings>
    <MultipleInstancesPolicy>IgnoreNew</MultipleInstancesPolicy>
    <DisallowStartIfOnBatteries>false</DisallowStartIfOnBatteries>
    <StopIfGoingOnBatteries>false</StopIfGoingOnBatteries>
    <AllowHardTerminate>true</AllowHardTerminate>
    <StartWhenAvailable>false</StartWhenAvailable>
    <RunOnlyIfNetworkAvailable>false</RunOnlyIfNetworkAvailable>
    <IdleSettings>
      <StopOnIdleEnd>true</StopOnIdleEnd>
      <RestartOnIdle>false</RestartOnIdle>
    </IdleSettings>
    <AllowStartOnDemand>true</AllowStartOnDemand>
    <Enabled>true</Enabled>
    <Hidden>false</Hidden>
    <RunOnlyIfIdle>false</RunOnlyIfIdle>
    <WakeToRun>false</WakeToRun>
    <ExecutionTimeLimit>PT0S</ExecutionTimeLimit>
    <Priority>7</Priority>
  </Settings>
  <Actions Context="Author">
    <Exec>
      <Command>C:\Users\MOTUS_USER\Projects\MOTUS_KIOSK\MOTUS_MSWINDOWS_STARTSERVER.bat</Command>
      <WorkingDirectory>C:\Users\MOTUS_USER\Projects\MOTUS_KIOSK\</WorkingDirectory>
    </Exec>
  </Actions>
</Task>
